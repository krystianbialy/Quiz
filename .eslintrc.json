{
    "extends": ["airbnb-base/legacy", "prettier"],
    "plugins": ["prettier"],
    "rules": {
        "prettier/prettier": "warn",
        "func-names":"off",
        "no-restricted-globals":"off",
        "vars-on-top":"off",
        "eqeqeq":"off"
        
    },
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    }
}